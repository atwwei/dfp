  "repository": {
    "type": "git",
    "url": "git@github.com:atwwei/dfp"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch false --code-coverage true",
    "lint": "eslint ./src --format junit --output-file ./reports/eslint.xml",
    "lint-fix": "eslint --fix ./src",
    "prettier": "prettier --check \"./**/*.{ts,js,json,md,html,scss,yml}\"",
    "prettier-fix": "prettier --write \"./**/*.{ts,js,json,md,html,scss,yml}\"",
    "publish": "npm run build && cd dist && node ../scripts/publish.js"
  },
  "dependencies": {
    "@types/googletag": "^1.1.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular-eslint/builder": "12.3.1",

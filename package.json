  },
  "repository": {
    "type": "git",
    "url": "git@github.com:atwwei/dfp"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && node ./scripts/build.js",
    "watch": "ng build --watch --configuration development",
